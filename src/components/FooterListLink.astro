---
interface Props {
  title: string,
  linkList: string[],
}
const {title, linkList} = Astro.props as Props;
---
<div class="text-white">
  <h2 class="text-bold text-base tracking-tightFooterHeading text-center mb-[1.375rem]">{title}</h2>
  <ul class="flex flex-col gap-[.625rem]">
    {linkList.map((linkItem) => (<li class="text-center text-secondarytxt text-sm tracking-tight">{linkItem}</li>) )}
  </ul>
</div>

  
