---
interface Props {
  title: string,
  desc: string,
  svg: HTMLElement,
}
const {title, desc, svg} = Astro.props;
---

<article>
    <h1>{title}</h1>
  <div class="absolute top-[-2.75rem] w-statsIcons h-statsIcons bg-secondary rounded-statsIcons flex items-center justify-center">{svg}</div>
    <p> {desc} </p>
</article>
